profiles {
	stub_run {
		process {
			executor = 'local'

			withName: '.*' {
				cpus = 1
				memory = '1G'
			}
		}
	}

	local {
		process {
			executor = 'local'
		}
	}

	ncpu {
		process {
			executor = 'slurm'
			time = '3d'
			cpus = 32
			memory = '256GB'
		}

		executor {
			queue = 'ncpu'
		}
	}
}
